{
  "name": "@pytorch-ignite/code-generator",
  "version": "0.3.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest --color --runInBand",
    "test:ci": "start-server-and-test --expect 200 serve http://127.0.0.1:5000 test",
    "release": "node scripts/release.js",
    "fmt": "prettier --write . && bash scripts/run_code_style.sh fmt",
    "min_lint": "prettier --check . && bash scripts/run_code_style.sh min_lint",
    "lint": "prettier --check . && bash scripts/run_code_style.sh lint"
  },
  "dependencies": {
    "@iconify/iconify": "^3.1.0",
    "@octokit/core": "^4.2.0",
    "@types/ejs": "^3.1.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.4.0",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.30",
    "ejs": "^3.1.6",
    "execa": "^7.1.1",
    "file-saver": "^2.0.5",
    "jest": "^27.5.0",
    "jszip": "^3.10.1",
    "playwright-chromium": "^1.33.0",
    "prettier": "^2.5.1",
    "prismjs": "^1.26.0",
    "prompts": "^2.4.2",
    "semver": "^7.3.5",
    "start-server-and-test": "^2.0.0",
    "uuid": "^9.0.0",
    "vite": "^2.7.13",
    "vue": "^3.2.30",
    "vue-router": "^4.0.12"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "none"
  }
}
