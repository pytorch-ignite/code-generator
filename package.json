{
  "name": "@pytorch-ignite/code-generator",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/getCommit.js && vite build",
    "serve": "vite preview",
    "black:fmt": "black src/templates -l 80",
    "black:lint": "black src/templates -l 80 --check",
    "isort:fmt": "isort src/templates --profile black",
    "isort:lint": "isort src/templates --profile black --check",
    "fmt": "prettier --write . && pnpm run black:fmt && pnpm run isort:fmt",
    "lint": "prettier --check . && pnpm run black:lint && pnpm run isort:lint"
  },
  "devDependencies": {
    "@iconify/iconify": "^2.0.1",
    "@types/ejs": "^3.0.6",
    "@types/file-saver": "^2.0.2",
    "@types/prismjs": "^1.16.5",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "conventional-changelog-cli": "^2.1.1",
    "ejs": "^3.1.6",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "prettier": "^2.3.0",
    "prismjs": "^1.23.0",
    "vite": "^2.3.2",
    "vue": "^3.0.11"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "none"
  }
}
